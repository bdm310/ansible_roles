vrrp_instance VI_1 {
    state {{ router_keepalive_state }}
    interface eth1
    virtual_router_id 51
    priority {{ router_keepalive_priority }}
    nopreempt
    advert_int 1
 
    virtual_ipaddress {
        # virtual IP address
        {{ router_virt_ip }}/{{ router_lan_prefix }} dev eth1
    }
}
